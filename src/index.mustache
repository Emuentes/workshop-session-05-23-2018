<body>
  <div class="tracing-image">
    {{> ./partials/top-nav/top-nav.mustache }}
    {{> ./partials/hero/hero.mustache }}
    {{> ./partials/square-btns/square-btns.mustache }}
    {{> ./partials/semantic-filter/semantic-filter.mustache }}
    {{> ./partials/info-tiles/info-tiles.amp.mustache }}
    {{> ./partials/get-started/get-started.mustache }}
    <div class="featured-articles horizontal-tiles scrolling-tiles-horizontal">
      <div class="featured-article">
